<div class="row">
  <%= render ExMoney.Web.SharedView, "settings_navbar.html", conn: @conn, navigation: @navigation %>

  <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
    <h2>Edit category</h2>

    <%= render "form.html",
      changeset: @changeset,
      action: settings_category_path(@conn, :update, @category),
      categories: @categories %>

    <%= link "Back", to: settings_category_path(@conn, :index) %>
  </div>
</div>
